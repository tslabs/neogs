        JR Z,LOADWT
        RRCA
        IN A,(ZXDATRD)
        JR C,LOADCM
        ADD A,C
        LD (HL),A
        INC L
        JP NZ,LOADWT
        INC H
        JP NZ,LOADWT
        INC E
        LD HL,0X8000
        JP LOAD_

LOADCM  IN A,(ZXCMD)
        CP 0XF3
        JP Z,COMF3
        CP 0XF4
