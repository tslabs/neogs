INITV01 PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        PUSH HL
        DJNZ INITV01
        LD SP,ISTACK
        LD (NUMPG),A
        EX AF,AF'
;---patched
        CALL Patch5i2
;---
        LD A,0X00
        LD (INFO),A
        XOR A
        LD (ROMPG),A
        LD HL,DAC0
        LD A,(HL)
        INC H
        LD A,(HL)
        INC H
        LD A,(HL)
        INC H
        LD A,(HL)
