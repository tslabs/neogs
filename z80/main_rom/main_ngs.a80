        OUT (MPAG),A
        RET

MLDI    NEG
        ADD A,A
        LD IYL,A
        LD A,HIGH (LDITAB)
        ADC A,0X00
        LD IYH,A
        LD A,(SDPAGE)
        LD (CPAGE),A
        OUT (MPAG),A
        JP (IY)

TLDI    NEG
        ADD A,A
        LD IYL,A
        LD A,HIGH (LDITAB)
        ADC A,0X00
        LD IYH,A
        SCF
        JP (IY)

MLDD    NEG
        ADD A,A
        LD IYL,A
        LD A,HIGH (LDDTAB)
        ADC A,0X00
        LD IYH,A
        LD A,(SDPAGE)
        LD (CPAGE),A
        OUT (MPAG),A
        JP (IY)

	align 256

LDDTAB  REPT 0X100
	DB 0XED,0XA8
