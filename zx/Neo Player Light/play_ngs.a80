
;LAST UPDATE 29.09.2014 savelij

		include macros.a80
	        include ports_ngs.a80

VERS_PLAYER	EQU "0.53"

GSCOD		EQU 0X8000			;     

OPISAT		EQU 0X6000			;      

SIZ_PAT		EQU 0X14

		ORG 0X8000
START		DI
		LD HL,0X0110
		LD (0X5C09),HL			;  
		LD (STEK),SP			; 
		LD SP,0
STEK		EQU $-2
		RES 3,(IY+48)
		RES 5,(IY+1)

;   CS =  TURBO OFF
		LD A,0XFE
		IN A,(0XFE)
		RRA
		LD A,0
		JR C,$+4
		LD A,0X10
		LD BC,0XEFF7
		OUT (C),A

;   SS = RESET NEOGS
		LD A,0X7F
		IN A,(0XFE)
		RRA
		RRA
		JR NC,LOOP21

;  GS/NEOGS
LOOP31		LD B,0X30			;    
		XOR A				; 0X1D   =0
		OUT (GSDAT),A			;  0X30 
		LD A,0X1D
		OUT (GSCOM),A
LOOP22		EI
		HALT
		DI
		DEC B
		JR Z,LOOP21			; ,    ?
		IN A,(GSCOM)			;   
		RRA
		JR C,LOOP22
		IN A,(GSDAT)			;  ,     
		XOR A				;     
		OUT (GSDAT),A			;    0XFF   
		LD A,0XFF
		LD B,0X30			;  0X30 
		OUT (GSCOM),A
LOOP001		EI
		HALT
		DI
		DEC B
		JR Z,LOOP21			; ,   
		IN A,(GSCOM)
		RRA
		JR C,LOOP001
		JR LOOP13			;   ,    

;  NEOGS
LOOP21		LD A,M_DBIT
		OUT (GSCTR),A			;  
		EI
		HALT
		HALT
		DI
		LD A,0XF3
		OUT (GSCOM),A			;   
		LD B,0X30			; 0X30 
LOOP25		EI
		HALT
		HALT
		DI
		DEC B
		JP Z,NO__GS			; ,  
		IN A,(GSCOM)
		RRA
		JR C,LOOP25			;  , 

;     NEOGS
LOOP24		LD A,0X23			;   
		OUT (GSCOM),A			;      
		CALL WC
		IN A,(GSDAT)
		OR 0X3F
		LD D,A
		LD E,MPAG			;   
		CALL OUTPORT			;      
		LD BC,GSDAT			;      
		LD DE,FAT_END-GO_YES
		LD HL,GSCOD
		OUT (C),E
		LD A,0X14
		OUT (GSCOM),A
		CALL WC
		OUT (C),D
		CALL WD
		OUT (C),L
		CALL WD
		OUT (C),H
		CALL WD
		LD HL,GO_YES
LODCOD		OUTI
		CALL WD
		DEC DE
		LD A,D
		OR E
		JR NZ,LODCOD
		LD HL,GSCOD			; , 
		OUT (C),L
		LD A,0X13
		OUT (GSCOM),A
		CALL WC
		OUT (C),H
		EI
		HALT
		HALT
		DI				; 2   

LOOP13		LD A,7
		CALL CLS			; 
		LD DE,0X4800
		LD HL,MESS20			;     
		CALL PRINT
		LD A,0X1D			;      
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		LD B,0
		CALL WAITGS			;   
		IN A,(GSDAT)			; 
		CP 0X8B	
		JR Z,LOOP24			; 0X8B    
		CP 0X77
		JR Z,Warm			;  0X77,    
		CP 0XEE
		JR NZ,LOOP17
		LD A,7
		CALL CLS
		LD HL,MESS13
		LD DE,0X5000
LOOP20		CALL PRINT
		LD HL,MESS15
		LD DE,0X5040
		CALL PRINT
		LD HL,MESS16
		LD DE,0X5080
		CALL PRINT
		JR LOOP16

LOOP17		CP 0XDD
		JR NZ,LOOP26
		LD A,7
		CALL CLS
		LD HL,MESS14
		LD DE,0X5000
		JR LOOP20

LOOP26		CP 0XBB
		JR NZ,LOOP27
		LD A,7
		CALL CLS
		LD HL,MESS23
		LD DE,0X5000
		JR LOOP20

LOOP27		CP 0XCC
		JR NZ,LOOP13
		JP NO_NGS

LOOP16		BIT 5,(IY+1)
		JR Z,LOOP16
		LD A,(IY-0X32)
		RES 5,(IY+1)
		CP 0X0D
		JP Z,START
		CP 0X20
		JP Z,RESCOMP
		JR LOOP16

Warm		LD A,7
		CALL CLS			; 
		CALL MORDALL			;  
		LD A,0X15
		LD DE,0X400D
		CALL READ5BT			; - 
		CALL NAMELNG			;   
		CALL DAT_VTS			;   
LOOP		XOR A
		CALL GET_VTS
		BIT 7,A
		JP NZ,EJECT
;		AND 5
;		CP 5
;		JR Z,LOOPM
		CALL FGETVTS
		LD A,H
		PUSH AF
		BIT 5,A
		CALL NZ,NAMELNG
		POP AF
		BIT 1,A
		JR NZ,LOOPM
		LD DE,0X486B
		CALL READSEC			; 
		CALL BITRATE
		JR LOOP14

LOOPM		CALL POSMOD
LOOP14		EI
		HALT
		DI
		BIT 5,(IY+1)
		JR Z,LOOP
		LD A,(IY-0X32)
		LD B,A
		RES 5,(IY+1)
		CALL FGETVTS
		LD A,H
		BIT 1,A
		LD HL,MODCOM
		JR NZ,LOOP14_1
		LD HL,MP3COM
LOOP14_1	LD DE,4
LOOP14_2	LD A,(HL)
		AND A
		JR Z,LOOP
		CP B
		JR Z,RDCOMM
		ADD HL,DE
		JR LOOP14_2

RDCOMM		INC HL
		LD A,(HL)
		INC HL
		LD E,(HL)
		INC HL
		LD D,(HL)
		EX DE,HL
		JP (HL)

MP3COM		DB "6",0X09
		DW EXE1
		DB "7",0X08
		DW EXE1
		DB "8",0X07
		DW EXE1
		DB "9",0X06
		DW EXE2
		DB 0X0D,0X0A
		DW EXE1
		DB ":",0X17
		DW EXE1
;		DB "q",0X03
;		DW EXE3
;		DB "a",0X01
;		DW EXE3
;		DB "w",0X0C
;		DW EXE3
;		DB "s",0X04
;		DW EXE3
;		DB "e",0X30
;		DW EXE3
;		DB "d",0X10
;		DW EXE3
;		DB "r",0XC0
;		DW EXE3
;		DB "f",0X40
;		DW EXE3

MODCOM		DB "1",0X01
		DW EXE1
		DB "2",0X02
		DW EXE4
		DB "3",0X03
		DW EXE4
		DB "4",0X04
		DW EXE4
		DB "5",0X05
		DW EXE1
		DB "!",0X18
		DW EXE1
		DB "%",0X19
		DW EXE1
		DB "0",0X00
		DW EJECT
		DB " ",0X00
		DW RESCOMP
		DB 0

EXE1		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		JP LOOP-3

EXE2		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		CALL PRTTBL
		JP LOOP-3

EXE3		PUSH AF
		LD A,0X1A
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		POP AF
		OUT (GSDAT),A
		CALL PRTTBL
		JP LOOP

EXE4		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		JP LOOP

;    
RESCOMP		CALL NANYKEY
		XOR A
		LD BC,0X7FFD
		OUT (C),A
		JP 0

WAITGS		LD E,B
		LD D,0
		LD HL,VENTIL
		ADD HL,DE
		LD A,(HL)
		LD DE,0X481B
		CALL PRISYM
		LD C,5
		EI
		HALT
		DI
		DEC C
		JR NZ,$-4
		IN A,(GSCOM)
		RRA
		RET NC
		INC B
		LD A,4
		CP B
		JR NZ,WAITGS
		LD B,0
		JR WAITGS

;      
NAMELNG		LD A,0X16
		LD DE,0X484D
		CALL READ5BT			;  
		LD HL,0X8000
		LD A,0X11
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC				;  
		LD A,H
		OUT (GSDAT),A
		CALL WD
		LD A,L
		OUT (GSDAT),A
;		CALL WD
		LD HL,OPISAT
		LD E,0
		PUSH HL
		CALL INI_E
		LD HL,0X5000
		LD D,H
		LD E,L
		LD BC,0X07FF
		INC E
		LD (HL),L
		LDIR
		POP HL
		LD B,0X20
		LD DE,0X5000
		CALL PRINT_B
		LD B,0X20
		LD DE,0X5020
		CALL PRINT_B
		LD B,0X20
		LD DE,0X5040
		CALL PRINT_B
		LD B,0X20
		LD DE,0X5060
		CALL PRINT_B
		LD B,0X20
		LD DE,0X5080
		CALL PRINT_B
		LD B,0X20
		LD DE,0X50A0
		CALL PRINT_B
		LD B,0X20
		LD DE,0X50C0
		CALL PRINT_B
		LD B,0X20
		LD DE,0X50E0
		CALL PRINT_B
		JP MORDAR

DAT_VTS		CALL FGETVTS		;  
		LD DE,0X48B0
		BIT 1,H
		RET NZ			; MOD
		BIT 7,H
		LD A,0X2B
		JR NZ,$+4
		LD A,"-"
		CALL PRISYM		;  TREBLE/BASS
		BIT 0,H
		LD A,"+"
		JR NZ,$+4
		LD A,"-"
		LD DE,0X4890
		CALL PRISYM		;  VIRTUAL SURROUND
		BIT 6,H
		LD A,"+"
		JR NZ,$+4
		LD A,"-"
		LD DE,0X48DF
		CALL PRISYM		;  MUTE ON/OFF
		LD A,L
		LD DE,0X48D0
		CALL PRIHEX

;  SOFT/HARD RESET
		BIT 4,H
		LD HL,MESS16+2
		JR Z,DATVTS1
		RES 5,(HL)
		INC HL
		RES 5,(HL)
		INC HL
		RES 5,(HL)
		INC HL
		RES 5,(HL)
		JR DATVTS2

DATVTS1		SET 5,(HL)
		INC HL
		SET 5,(HL)
		INC HL
		SET 5,(HL)
		INC HL
		SET 5,(HL)
DATVTS2		LD HL,MESS16
		LD DE,0X4014
		CALL PRINT

;    
PRTTBL		
;		CALL FGETVTS
;		BIT 3,H
;		JR Z,EMPTSTR
;		BIT 7,H
;		JR Z,EMPTSTR
;		LD A,0X1B
;		OUT (GSDAT),A
;		LD A,0X1F
;		OUT (GSCOM),A
;		CALL WC
;		LD HL,OPISAT+0X0100
;		LD E,8
;		PUSH HL
;		CALL INI_E
;		LD DE,0X48B1
;		LD B,2
;		POP HL
;		CALL PRINT_B
;		LD A,"0"
;		CALL PRISYM
;		LD A,"0"
;		CALL PRISYM
;		LD A,"0"
;		CALL PRISYM
;		LD A,"/"
;		CALL PRISYM
;		LD B,2
;		CALL PRINT_B
;		INC E
;		LD B,2
;		CALL PRINT_B
;		LD A,"0"
;		CALL PRISYM
;		LD A,"/"
;		CALL PRISYM
;		LD B,2
;		JP PRINT_B

;  
EMPTSTR		LD HL,EMPT16+1
		LD DE,0X48B1
		JP PRINT

;    
BITRAT1		LD HL,EMPT16+2
		LD DE,0X4856
		PUSH HL
		LD B,5
		CALL PRINT_B
		LD DE,0X4878
		LD B,3
		POP HL
		PUSH HL
		CALL PRINT_B
		POP HL
		LD B,0X0C
		LD DE,0X4834
		JP PRINT_B

; MP3 
BITRATE		LD A,0X1C
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		CALL WN
		IN A,(GSDAT)
		LD B,A
		CALL WN
		IN A,(GSDAT)
		LD C,A
		LD A,B
		AND 0XE0
		CP %10100000
		JR Z,BITRAT1
		LD A,C
		AND 3
		LD L,A
		ADD A,A
		ADD A,A
		ADD A,L
		LD L,A
		LD H,0
		LD A,C
		RRCA
		RRCA
		AND 3
		LD DE,SR0
		JR Z,BITRAT3
		DEC A
		LD DE,SR1
		JR Z,BITRAT3
		LD DE,SR2
BITRAT3		ADD HL,DE
		LD DE,0X4856
		PUSH BC
		LD B,5
		CALL PRINT_B
		POP BC
		LD A,C
		RLCA
		RLCA
		AND 3
		ADD A,A
		ADD A,A
		LD L,A
		ADD A,A
		ADD A,L
		LD L,A
		LD H,0
		LD DE,REJCHAN
		ADD HL,DE
		PUSH BC
		LD B,0X0C
		LD DE,0X4834
		CALL PRINT_B
		POP BC
		LD A,B
		AND 0X0F
		LD L,A
		ADD A,A
		ADD A,L
		LD L,A
		LD H,0
		LD A,C
		AND 3
		CP 3
		JR NZ,BITRAT4
		LD A,0X30
		ADD A,L
		LD L,A
BITRAT4		LD A,C
		AND 0X30
		SUB 0X10
		LD DE,L3ID0
		JR Z,BITRAT2
		SUB 0X10
		LD DE,L2ID0
		JR Z,BITRAT2
		LD DE,L1ID0
BITRAT2		ADD HL,DE
		LD B,3
		LD DE,0X4878
		JP PRINT_B

;   -   (15)
;    (16)
READ5BT		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		JP PRISYM

;   
;   
READSEC		LD A,0X0B
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		LD A,0X3A
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		LD A,0X3A
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		CALL PRISYM
		CALL WN
		IN A,(GSDAT)
		JP PRISYM

;  NEOGS   128 
NO_NGS		LD A,7
		CALL CLS
		LD DE,0X4809
		LD HL,MESS17
		CALL PRINT
		LD DE,0X4846
		LD HL,MESS19
		CALL PRINT
		CALL NANYKEY
		CALL ANYKEY
		CALL NANYKEY
BAS128		XOR A
		LD BC,0X7FFD
		OUT (C),A
		JP 0

;  
ANYKEY		XOR A
		IN A,(0XFE)
		CPL
		AND 0X1F
		JR Z,$-6
		RET

;  
NANYKEY		XOR A
		IN A,(0XFE)
		CPL
		AND 0X1F
		JR NZ,$-6
		RET

;  GS   128 
NO__GS		LD A,7
		CALL CLS
		LD DE,0X480A
		LD HL,MESS18
		CALL PRINT
		LD DE,0X4846
		LD HL,MESS19
		CALL PRINT
		CALL ANYKEY
		CALL NANYKEY
		JR BAS128

;    0   B  
PRINT_B		LD A,(HL)
		AND A
		RET Z
		INC HL
		CALL PRISYM
		DJNZ PRINT_B
		RET

;    0
PRINT		LD A,(HL)
		INC HL
		AND A
		RET Z
		CALL PRISYM
		JR PRINT

MORDALL		LD HL,MESS1
		LD DE,0X4000
		CALL PRINT
		LD HL,MESS16
		LD DE,0X4014
		CALL PRINT
		LD HL,MESS2
		LD DE,0X4040
		CALL PRINT
		LD HL,MESS3
		LD DE,0X4060
		CALL PRINT
		LD HL,MESS4
		LD DE,0X4080
		CALL PRINT
		LD HL,MESS5
		LD DE,0X40A0
		CALL PRINT
		LD HL,MESS6
		LD DE,0X40C0
		CALL PRINT
		LD HL,MESS7
		LD DE,0X4800
		CALL PRINT
		LD HL,MESS8
		LD DE,0X4840
		CALL PRINT
		LD HL,MESS24
		LD DE,0X48E0+((32-(EMESS24-MESS24))/2)
		CALL PRINT
		LD A,3
		CALL GET_VTS
		LD HL,0X59E0
		LD B,0X20
		AND 0X0C
		LD C,A
		XOR 0X0C
		ADD A,A
		OR 0X21
		LD (HL),A
		INC L
		DJNZ $-2
		LD A,C
		RRCA
		RRCA
		ADD A,"0"		;  MP3 
		LD DE,0X48FF
		CALL PRISYM
MORDAR		LD A,3
		CALL GET_VTS
		BIT 1,A
		JR NZ,MORDMOD
		LD HL,MESS9
		LD DE,0X4860
		CALL PRINT
		LD HL,MESS10
		LD DE,0X48A0
		CALL PRINT
		LD HL,MESS11
		LD DE,0X4880
		CALL PRINT
		LD HL,MESS12
		LD DE,0X48C0
		CALL PRINT
		LD HL,MESSEM
		LD DE,0X48D3
		CALL PRINT
		LD HL,MESS26
		LD DE,0X485C
		CALL PRINT
		LD HL,MESS27
		LD DE,0X487C
		CALL PRINT
		JP PRTTBL

MORDMOD		LD HL,EMPT16+3
		LD DE,0X4853
		CALL PRINT
		LD HL,EMPT16
		LD DE,0X4890
		CALL PRINT
		LD HL,EMPT16
		LD DE,0X48B0
		CALL PRINT
		LD HL,EMPT16
		LD DE,0X4830
		CALL PRINT
		LD HL,MOD00
		LD DE,0X4860
		CALL PRINT
		LD HL,EMPT16
		LD DE,0X4870
		CALL PRINT
		LD HL,MOD02
		LD DE,0X4880
		CALL PRINT
		LD HL,MOD03
		LD DE,0X48A0
		CALL PRINT
		LD HL,EMPT16
		LD DE,0X48D0
		CALL PRINT
		LD HL,EMPT16
		LD DE,0X48C0
		JP PRINT

POSMOD		LD A,SIZ_PAT
		CALL GET_VTS
		LD DE,0X486E
		CALL PRIHEX
		LD DE,0X488E
		LD A,0X60
		OUT (GSCOM),A
		CALL WC
		IN A,(GSDAT)
		CALL PRIHEX
		LD DE,0X48AE
		LD A,0X61
		OUT (GSCOM),A
		CALL WC
		IN A,(GSDAT)
		JP PRIHEX

;      "A"
GET_VTS		OUT (GSDAT),A
		LD A,0XFF
		OUT (GSCOM),A
		CALL WC
;		CALL WAIT_WN
		IN A,(GSDAT)
		RET

; 16  
FGETVTS		LD A,0X0C
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		CALL WN
		IN A,(GSDAT)
		LD H,A
		CALL WN
		IN A,(GSDAT)
		LD L,A
		RET

MOD00		DB "Length:        ",0
MOD02		DB "Position:      ",0
MOD03		DB "Note:          ",0

VENTIL		DB 0XC4,0X5C,0X7C,0X2F

MESS1		DB "Found files:",0					;0X4000 0X400b
MESS16		DB 0X22,"Space",0X22," Exit",0				;0X4014
MESS2		DB "1 Prev. File  SS+1 Prev. DIR",0
MESS3		DB "2 Play File",0
MESS4		DB "3 Pause",0
MESS5		DB "4 Stop",0
MESS6		DB "5 Next File   SS+5 Next DIR",0
MESS7		DB "0 Eject SD Card",0
MESS8		DB "Play Number:",0					;0X4840 0X484D
MESS9		DB "Time Play:      ",0					;0X4860 0X486B
MESS10		DB "9 Treble/Bass:  ",0					;0X4880 0X4890
MESS11		DB "8 Surround:     ",0					;0X48A0 0X48Ad
MESS12		DB "<6-7> Volume:   ",0
MESSEM		DB 0X22,"Enter",0X22," Mute",0				;0X48C0 0X48CE
MESS13		DB "SD-Card Not Found",0				;0X5000
MESS14		DB "FAT not Found",0					;0X5000
MESS15		DB "Press ",0X22,"ENTER",0X22," 4 Restart Init SD",0
MESS17		DB "Neo"
MESS18		DB "GS not found",0
MESS19		DB "Press any key for reset",0
MESS20		DB "Please wait, search files",0
MESS21		DB "Eject SD card or insert new card",0
