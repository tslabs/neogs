EJECT		LD A,4
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		CALL WC
		LD A,7
		CALL CLS
		LD HL,MESS21
		LD DE,0X4800
		CALL PRINT
		LD HL,MESS22
		LD DE,0X4849
		CALL PRINT
		CALL NANYKEY
		CALL ANYKEY
		CALL NANYKEY
		XOR A
		OUT (GSDAT),A
		LD A,0X1F
		OUT (GSCOM),A
		JP LOOP13

CLS		LD HL,0X4000
		LD E,L
		LD D,H
		LD (HL),L
		INC E
		LD BC,0X1800
		LDIR
CLSCLR		LD (0X5C8D),A
		LD (0X5C48),A
		LD HL,0X5800
		LD D,H
		LD E,L
		LD (HL),A
